    "import sys\n",
    "\n",
    "k  = int(input())\n",
    "num = []\n",
    "\n",
    "for i in range(k):\n",
    "    a = int(input(sys.stdin.readline()))\n",
    "    if a == 0:\n",
    "        if len(num) == 0:\n",
    "            pass\n",
    "        else:\n",
    "            num.pop(-1)\n",
    "    else:\n",
    "        num.append(a)\n",
    "        \n",
    "print(sum(num))"
   ]
